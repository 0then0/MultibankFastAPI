# Application
PROJECT_NAME=Multibank Aggregator
VERSION=0.1.0
API_V1_STR=/api/v1

# Security
SECRET_KEY=change-this-to-a-random-secret-key-min-32-chars
ALGORITHM=HS256
ACCESS_TOKEN_EXPIRE_MINUTES=30
REFRESH_TOKEN_EXPIRE_DAYS=7

# Database
POSTGRES_SERVER=localhost
POSTGRES_PORT=5432
POSTGRES_USER=postgres
POSTGRES_PASSWORD=postgres
POSTGRES_DB=multibank

# CORS
BACKEND_CORS_ORIGINS=["http://localhost:3000","http://localhost:8000","http://localhost:5173"]

# Banking API
BANKING_API_BASE_URL=https://vbank.open.bankingapi.ru
BANKING_API_CLIENT_ID=your-client-id
BANKING_API_CLIENT_SECRET=your-client-secret
BANKING_API_TOKEN=your-api-token

# Rate limiting
RATE_LIMIT_PER_MINUTE=60

# Logging
LOG_LEVEL=INFO
